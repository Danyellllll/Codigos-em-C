#include <stdio.h>


int main () {

    double x, y, z;

    printf("Digite dois numeros: \n");
    scanf("%lf", &x);
    scanf("%lf", &y);

    while (x != y){
        if (x > y){
            printf("DECRESCENTE! \n");
        }
        else{
            printf("CRESCENTE! \n");
        }
        printf("Digite dois numeros: \n");
        scanf("%lf", &x);
        scanf("%lf", &y);
    }



    return 0;
}

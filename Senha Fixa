#include <stdio.h>


int main () {

    double x, y, z, a;

    z = 2002;

    printf("Digite a senha: ");
    scanf("%lf", &x);

    while (x != z){
        printf("Senha Invalida! Tente Novamente: ");
        scanf("%lf", &x);
    }
    if(x = y){
        printf("Acesso Permitido");
    }

    return 0;
}
